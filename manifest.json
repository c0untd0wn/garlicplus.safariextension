{
  "manifest_version": 2,

  "name": "GarlicPlus Image Search",
  "description": "Search/Download Image with Different Resolution using Google Image Search",
  "version": "1.0",

  "permissions": [
    "contextMenus",
    "tabs",
    "downloads",
    "http://*.google.com/",
    "https://*.google.com/"
  ],
  "background": {
    "scripts": ["javascript/script.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
